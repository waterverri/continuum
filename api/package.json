    "jwks-rsa": "^3.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express-jwt": "^6.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.3",
    "gts": "^5.3.0",
